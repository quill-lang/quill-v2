(module
  ()
  (def id (u)
    (pi T imp (sort (univsucc (univvar u)))
      (pi x ex (bound 0)
        (bound 1)))
    (lam T imp (sort (univsucc (univvar u)))
      (lam x ex (bound 0)
        (bound 0)))
  )
  (ind Bool () () (sort (univsucc (univzero))) (
    (true (inst (Bool) ()))
    (false (inst (Bool) ()))
  ))
)
